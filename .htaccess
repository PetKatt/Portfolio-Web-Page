### PROTECT FILES ###
<FilesMatch "\.(htaccess|htpasswd|ini|fla|psd|log|sh)$">
Order Allow,Deny
Deny from all
</FilesMatch>

RewriteEngine on
RewriteRule zatrudnij-programiste-webowego index.html
RewriteRule kim-jestem who.html
RewriteRule oferta-tworzenie-stron-www offer.html
RewriteRule cv-programista-php-javascript cv.html
RewriteRule kontakt-programista-php contact.html